{
  "name": "@openutility/app-data-client",
  "version": "1.0.0",
  "description": "library for processing flags on the client",
  "main": "dist/index.js",
  "scripts": {
    "test": "tsc --project ./tsconfig.test.json && jasmine",
    "build": "npm run build:js && npm run build:types",
    "build:js": "rollup -c rollup.config.js",
    "build:types": "tsc -t esnext --moduleResolution node -d --emitDeclarationOnly --outFile dist/core.d.ts src/index.ts",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openUtility/app-data-client-js.git"
  },
  "keywords": [
    "flags",
    "switches",
    "switch"
  ],
  "author": "andrewjrvs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/openUtility/app-data-client-js/issues"
  },
  "homepage": "https://github.com/openUtility/app-data-client-js#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/jasmine": "^4.3.0",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "node-fetch": "^3.2.10",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  }
}
